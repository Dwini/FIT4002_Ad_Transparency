        },
        {
          "name": "USE_PROXIES",
          "value": "0"
        },
        {
          "name": "CHANGE_LOCATION",
          "value": "1"
        },
        {
          "name": "DB_URL",
          "value": "http://3.234.242.222:8080"
        },
        {
          "name": "NUM_TERMS",
          "value": "10"
        },
        {
          "name": "UPLOAD_LOGS",
          "value": "1"
        }
      ]
    },
    {
      "name": "amydivers407",
      "image": "mattbertoncello/ad_transparency_bot",
      "essential": false,
