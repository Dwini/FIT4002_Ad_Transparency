{
  "family": "ad-transparency-batch3",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "memory": "8192",
  "cpu": "4096",
  "executionRoleArn": "ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "damiandarsey",
      "image": "mattbertoncello/ad_transparency_bot",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ad-transparency",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "AD_USERNAME",
          "value": "damiandarsey"
        },
        {
          "name": "USE_PROXIES",
          "value": "0"
        },
        {
          "name": "CHANGE_LOCATION",
          "value": "1"
        },
        {
          "name": "DB_URL",
          "value": "http://3.234.242.222:8080"
        },
        {
          "name": "NUM_TERMS",
          "value": "20"
        },
        {
          "name": "UPLOAD_LOGS",
          "value": "1"
        }
      ]
    },
    {
      "name": "kanderso922",
      "image": "mattbertoncello/ad_transparency_bot",
      "essential": false,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
